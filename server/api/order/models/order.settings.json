{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "Order"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "customer": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "order"
    },
    "restaurant": {
      "via": "orders",
      "model": "restaurant"
    },
    "orderedAt": {
      "type": "datetime"
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "orderItems": {
      "via": "order",
      "collection": "order-item"
    },
    "statuses": {
      "via": "order",
      "collection": "order-status"
    }
  }
}
